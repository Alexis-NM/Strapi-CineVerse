{
    "collectionName": "actors",
    "info": {
        "singularName": "actor",
        "pluralName": "actors",
        "displayName": "Actor"
    },
    "options": {
        "draftAndPublish": false
    },
    "attributes": {
        "tmdbId": {
            "type": "integer",
            "unique": true
        },
        "firstName": {
            "type": "string",
            "required": true
        },
        "lastName": {
            "type": "string",
            "required": true
        },
        "birthDate": {
            "type": "date"
        },
        "movies": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "api::movie.movie",
            "mappedBy": "actors"
        }
    }
}